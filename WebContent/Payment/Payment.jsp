  <body onload="createCartProduct(list);">

    <script>
 
		    function thanhtoan(){
		        window.location.href="${pageContext.request.contextPath}/Bill";
		    }
		    function tongtien()
			{
				var i,tong=0;
				for (i = 0; i < list.length; i++) {
				  tong=tong+list[i].price*list[i].soluong;
				}
				document.getElementById("paylabel").innerText= tong+17000;
			}
            function createCartProduct(listOfCartProducts){
                var i;
                var n=listOfCartProducts.length;
                for (i=0;i<n;i++)
                { 

                    var div2=document.createElement("div");
                    div2.setAttribute("class","pay-product product-selected");
                    div2.setAttribute("style","display:flex;width:100%;border: lavender;border-width: 0.01em;border-style: double;")

                    var div3=document.createElement("div");
                    div3.setAttribute("class","pay-item-image");
                    //div3.setAttribute("style","width:20%")
                    //  div3.setAttribute("style","padding-left:5%")
                    div2.appendChild(div3);

                    var image1=document.createElement("img");
                    image1.setAttribute("class","image-item");
                    image1.setAttribute("style","margin-left: 45%;")
                    image1.setAttribute("src",String(listOfCartProducts[i].imagesrc));

                    div3.appendChild(image1);


                    var div4=document.createElement("div");
                    //div4.setAttribute("style","width:20%")

                    div2.appendChild(div4);

                    var lable1= document.createElement("lable");
                    var lable1dis=document.createTextNode(String(listOfCartProducts[i].name));
                    lable1.appendChild(lable1dis);

                    div4.appendChild(lable1);
                    div2.appendChild(div4);

					
                    var div5=document.createElement("div");
                    var span1= document.createElement("span");
                   // div5.setAttribute("style","width:15%");
                    var span1dis=document.createTextNode("s");
                    span1.appendChild(span1dis);

                    div5.appendChild(span1);
                    div2.appendChild(div5);
				
                    var div6=document.createElement("div");
                   // div6.setAttribute("style","width:18%");
                    var span2= document.createElement("span");
                    var span2dis=document.createTextNode(String(listOfCartProducts[i].price));
                    span2.appendChild(span2dis);

                    div6.appendChild(span2);
                    div2.appendChild(div6);
					
                   
                    var div7=document.createElement("div");
                    //div7.setAttribute("style","width:15%");
                    var span3= document.createElement("span");
                    var span3dis=document.createTextNode(String(listOfCartProducts[i].soluong));
                    span3.appendChild(span3dis);

                    div7.appendChild(span3);
                    div2.appendChild(div7);
                    
                    var div8=document.createElement("div");
                    //div8.setAttribute("style","width:20%");
                    var span4= document.createElement("span");
                    var span4dis=document.createTextNode(String(listOfCartProducts[i].price*listOfCartProducts[i].soluong));
                    span4.appendChild(span4dis);

                    div8.appendChild(span4);
                    div2.appendChild(div8);
					
                    var input=document.createElement("input");
                    input.setAttribute("type","hidden");
                    input.setAttribute("value",listOfCartProducts[i].id_sp);
                    input.setAttribute("name","id"+listOfCartProducts.indexOf(listOfCartProducts[i]));
                    input.setAttribute("form","form1");
                    div2.appendChild(input);
                    
                    var soluong=document.createElement("input");
                    soluong.setAttribute("type","hidden");
                    soluong.setAttribute("value",listOfCartProducts[i].soluong);
                    soluong.setAttribute("name","soluong"+listOfCartProducts.indexOf(listOfCartProducts[i]));
                    soluong.setAttribute("form","form1");
                    div2.appendChild(soluong);
                    
                    
                    var parent=document.getElementById("items");
                    parent.appendChild(div2);
                    
                }
                tongtien();
                var tong=document.createElement("input");
                tong.setAttribute("type","hidden");
                tong.setAttribute("value",n);
                tong.setAttribute("name","tong");
                tong.setAttribute("form","form1");
                var parent=document.getElementById("items");
                parent.appendChild(tong);
            }
    </script>
